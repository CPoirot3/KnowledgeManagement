<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY eg   'http://bupt.edu.cn:eg/'>
    <!ENTITY rdf  'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
    <!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
    <!ENTITY xsd  'http://www.w3.org/2001/XMLSchema#'>
    <!ENTITY owl  "http://www.w3.org/2002/07/owl#" >
]>

<rdf:RDF xmlns:rdf="&rdf;" xmlns:rdfs="&rdfs;" xmlns:xsd="&xsd;" xmlns:owl="&owl;"
         xml:base="urn:x-hp:eg/" xmlns:eg="&eg;" xmlns="&eg;">

  <owl:Class rdf:about="&eg;Alarm">
      <rdfs:subClassOf>
          <rdf:Description>
             <owl:intersectionOf rdf:parseType="Collection">
                <owl:Restriction>
                   <owl:onProperty rdf:resource="&eg;hasEvent"/>
                   <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">10000</owl:minCardinality>
                </owl:Restriction>
              </owl:intersectionOf>
           </rdf:Description>
       </rdfs:subClassOf>
       <rdfs:subClassOf>
           <rdf:Description>
              <owl:intersectionOf rdf:parseType="Collection">
                 <owl:Restriction>
                    <owl:onProperty rdf:resource="&eg;hasEvent"/>
                    <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">9999</owl:maxCardinality>
                 </owl:Restriction>
               </owl:intersectionOf>
            </rdf:Description>
        </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="&eg;Event">
      <rdfs:subClassOf>
          <owl:Class rdf:about="&eg;A"/>
      </rdfs:subClassOf>
  </owl:Class>

  <owl:ObjectProperty rdf:about="&eg;hasEvent">
      <rdfs:range rdf:resource="&eg;Event" />
      <rdfs:domain rdf:resource="&eg;Alarm" />
  </owl:ObjectProperty>



</rdf:RDF>
